<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="book" class="com.edureka.beaninheritance.Book">
		<property name="name" value="mastering c++"></property>
		<property name="author" value="venugopal"></property>
		<property name="noOfPages" value="1000"></property>
		<property name="cost" value="500"></property>
	</bean>
	<bean id="ebook" class="com.edureka.beaninheritance.Ebook"
		parent="book">
		<property name="sizeInMB" value="25.2"></property>
	</bean>
</beans>
